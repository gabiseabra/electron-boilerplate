{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "version": "2.1.0",
  "description": "Electron + React + Redux + Webpack 2 boilerplate",
  "main": "build/app/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "prewatch": "yarn run build:main",
    "watch": "webpack-dev-server --config=./webpack/devServer.babel.js --hot --inline",
    "build": "webpack --config=webpack/config.babel.js",
    "build:test": "cross-env NODE_ENV=test yarn run build --",
    "build:dev": "cross-env NODE_ENV=development yarn run build --",
    "build:prod": "cross-env NODE_ENV=production yarn run build --",
    "build:main": "webpack --config=webpack/bundles/main.babel.js",
    "build:renderer": "webpack --config=webpack/bundles/renderer.babel.js",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "electron-mocha \"{test,src}/**/*.spec.js\"",
    "test:e2e": "mocha test/e2e/test.js",
    "lint": "eslint src test scripts",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "dist:mac": "electron-builder --mac",
    "dist:linux": "electron-builder --linux",
    "publish": "electron-builder --publish onTagOrDraft",
    "translate": "babel-node --presets es2015 -- ./scripts/translate.js -o ./src/locales/translations -l en",
    "clean": "yarn run clean:build & yarn run clean:dist",
    "clean:build": "rimraf ./build/app",
    "clean:dist": "rimraf ./dist"
  },
  "author": "Gabriela Seabra",
  "license": "MIT",
  "copyright": "Â© 2017 - Gabriela Seabra",
  "homepage": "https://github.com/gabiseabra/electron-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabiseabra/electron-boilerplate.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "devtron": "^1.4.0",
    "dotenv": "^4.0.0",
    "electron": "1.7.6",
    "electron-builder": "^19.27.2",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-mocha": "^4.0.2",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "flat": "^4.0.0",
    "glob": "^7.1.2",
    "google-fonts-webpack-plugin": "^0.4.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "mocha": "^3.5.0",
    "postcss": "^6.0.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^6.0.4",
    "prop-types": "^15.5.10",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "stylelint": "^7.8.0",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "util.promisify": "^1.0.0",
    "webpack": "^3.5.5",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-bundle-analyzer": "^2.9.0",
