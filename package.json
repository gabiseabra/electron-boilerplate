  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "intl": "^1.2.5",
    "is-electron-renderer": "^2.0.1",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "semver-utils": "^1.1.1"
  }
}
