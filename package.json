{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "version": "1.0.0",
  "description": "Electron + React + Redux + Webpack 2 boilerplate",
  "main": "build/app/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "prewatch": "yarn run build:main",
    "watch": "webpack-dev-server --config=./webpack/devServer.babel.js --hot --inline",
    "build": "webpack --config=webpack/config.babel.js",
    "build:dev": "cross-env NODE_ENV=development yarn run build --",
    "build:prod": "cross-env NODE_ENV=production yarn run build --",
    "build:main": "webpack --config=webpack/bundles/main.babel.js",
    "build:renderer": "webpack --config=webpack/bundles/renderer.babel.js",
    "lint": "eslint src",
    "pack": "build --dir",
    "dist": "build",
    "dist:win": "build --win",
    "dist:mac": "build --mac",
    "dist:linux": "build --linux",
    "translate": "babel-node --presets es2015 -- ./scripts/translate.js -o ./src/locales/translations -l en",
    "clean": "yarn run clean:build & yarn run clean:dist",
    "clean:build": "rimraf ./build/app",
    "clean:dist": "rimraf ./dist"
  },
  "author": "Gabriela Seabra",
  "license": "MIT",
  "copyright": "Â© 2017 - Gabriela Seabra",
  "homepage": "https://github.com/gabiseabra/electron-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabiseabra/electron-boilerplate.git"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.22.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "devtron": "^1.4.0",
    "dotenv": "^4.0.0",
    "electron": "^1.6.2",
    "electron-builder": "^18.1.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "google-fonts-webpack-plugin": "^0.3.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "material-design-icons": "^3.0.1",
    "postcss": "^6.0.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-url": "^6.0.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "stylelint": "^7.8.0",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "util.promisify": "^1.0.0",
    "webpack": "^2.6.1",
    "webpack-assets-manifest": "^0.6.2",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4",
    "yargs": "^8.0.2"
  },
  "optionalDependencies": {
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3"
  },
  "dependencies": {
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.22.0",
    "fs": "0.0.1-security",
    "intl": "^1.2.5",
    "is-electron-renderer": "^2.0.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.7",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "semver-utils": "^1.1.1"
  }
}
